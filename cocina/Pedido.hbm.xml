<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cocina" namespace="cocina">
	<class name="Pedido" table="Pedidos">
		<id name="Id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="Mesa" column="mesa" type="int" />
		<property name="Plato" column="plato" type="string" />
		<property name="Nota" column="nota" type="string" />
		<property name="Preparando" column="preparando" type="bool" />
		<property name="Entregado" column="entregado" type="datetime" />
	</class>
</hibernate-mapping>
