<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="cocina" namespace="cocina">
	<class name="Pedido" table="eskaera">
		<id name="Id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="Mesa" column="mahaila_id" type="int" />
		<property name="Nota" column="nota" type="string" />
		<property name="Preparando" column="egoera" type="bool" />
		<property name="Done" column="done" type="bool" />
		<property name="DoneAt" column="done_at" type="datetime" />

		<many-to-one name="Plato" column="platera" class="cocina.Platera" />
	</class>
</hibernate-mapping>
